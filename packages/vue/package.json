{
  "name": "@kossabos/vue",
  "license": "MIT",
  "description": "Kossabos Vue components",
  "version": "0.0.4",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "pnpx run build && pnpx run docs:dev",
    "publish": "pnpx publish --access public",
    "docs:dev": "vitepress dev docs --host --port 3702",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@primevue/themes": "^4.2.5",
    "@vueuse/core": "^12.4.0",
    "@vueuse/motion": "^2.2.6",
    "@vueuse/rxjs": "^12.4.0",
    "heroicons": "^2.2.0",
    "lucide-static": "^0.473.0",
    "lucide-vue-next": "^0.473.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.5",
    "tailwindcss-primeui": "^0.4.0",
    "vue": "^3.5.13",
    "vue-draggable-plus": "^0.6.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.37.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitepress": "^1.6.1"
  },
  "exports": {
    ".": {
      "main": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./dist/index.css"
  },
  "publishConfig": {
    "main": "./index.mjs",
    "module": "./index.mjs",
    "types": "./index.d.ts",
    "exports": {
      ".": {
        "types": "./index.d.ts",
        "import": "./index.mjs",
        "default": "./index.mjs"
      },
      "./*": {
        "types": "./*/index.d.ts",
        "import": "./*/index.mjs",
        "default": "./*/index.mjs"
      }
    },
    "directory": "dist",
    "linkDirectory": false,
    "access": "public"
  }
}