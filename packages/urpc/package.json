{
    "name": "@urpc/clients",
    "version": "0.0.1",
    "main": "./dist/src/index.js",
    "types": "./dist/src/index.d.ts",
    "type": "module",
    "bin": {
        "urpc-mcp": "./dist/mcp/main.js"
    },
    "exports": {
        ".": {
            "import": "./dist/src/index.js",
            "require": "./dist/src/index.js",
            "types": "./dist/src/index.d.ts"
        },
        "./core": {
            "import": "./dist/src/core/index.js",
            "require": "./dist/src/core/index.js",
            "types": "./dist/src/core/index.d.ts"
        },
        "./mcp": {
            "import": "./dist/src/core/mcp.js",
            "require": "./dist/src/core/mcp.js",
            "types": "./dist/src/core/mcp.d.ts"
        },
        "./clients": {
            "import": "./dist/src/clients/index.js",
            "require": "./dist/src/clients/index.js",
            "types": "./dist/src/clients/index.d.ts"
        },
        "./gh": {
            "import": "./dist/src/clients/gh.js",
            "require": "./dist/src/clients/gh.js",
            "types": "./dist/src/clients/gh.d.ts"
        },
        "./git": {
            "import": "./dist/src/clients/git.js",
            "require": "./dist/src/clients/git.js",
            "types": "./dist/src/clients/git.d.ts"
        },
        "./aws": {
            "import": "./dist/src/clients/aws.js",
            "require": "./dist/src/clients/aws.js",
            "types": "./dist/src/clients/aws.d.ts"
        },
        "./ffmpeg": {
            "import": "./dist/src/clients/ffmpeg.js",
            "require": "./dist/src/clients/ffmpeg.js",
            "types": "./dist/src/clients/ffmpeg.d.ts"
        },
        "./tar": {
            "import": "./dist/src/clients/tar.js",
            "require": "./dist/src/clients/tar.js",
            "types": "./dist/src/clients/tar.d.ts"
        },
        "./grep": {
            "import": "./dist/src/clients/grep.js",
            "require": "./dist/src/clients/grep.js",
            "types": "./dist/src/clients/grep.d.ts"
        },
        "./find": {
            "import": "./dist/src/clients/find.js",
            "require": "./dist/src/clients/find.js",
            "types": "./dist/src/clients/find.d.ts"
        },
        "./diff": {
            "import": "./dist/src/clients/diff.js",
            "require": "./dist/src/clients/diff.js",
            "types": "./dist/src/clients/diff.d.ts"
        },
        "./curl": {
            "import": "./dist/src/clients/curl.js",
            "require": "./dist/src/clients/curl.js",
            "types": "./dist/src/clients/curl.d.ts"
        },
        "./datadog": {
            "import": "./dist/src/clients/datadog.js",
            "require": "./dist/src/clients/datadog.js",
            "types": "./dist/src/clients/datadog.d.ts"
        },
        "./http": {
            "import": "./dist/src/core/http.js",
            "require": "./dist/src/core/http.js",
            "types": "./dist/src/core/http.d.ts"
        }
    },
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "mcp": "tsx mcp/main.ts",
        "mcp:stdio": "tsx mcp/main.ts --stdio"
    },
    "dependencies": {
        "minisearch": "^7.2.0",
        "zod": "^3.23.0"
    },
    "devDependencies": {
        "@koa/router": "^12.0.1",
        "@modelcontextprotocol/sdk": "^1.24.3",
        "@types/koa": "^2.14.0",
        "@types/koa-bodyparser": "^4.3.12",
        "@types/koa__router": "^8.0.11",
        "@types/node": "^20.0.0",
        "koa": "^2.15.0",
        "koa-bodyparser": "^4.4.1",
        "tsx": "^4.7.0",
        "typescript": "^5.0.0"
    }
}