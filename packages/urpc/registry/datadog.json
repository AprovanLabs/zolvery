{
  "restApi": {
    "datadog": {
      "openapi": {
        "v1": "https://raw.githubusercontent.com/DataDog/datadog-api-client-python/refs/heads/master/.generator/schemas/v1/openapi.yaml",
        "v2": "https://raw.githubusercontent.com/DataDog/datadog-api-client-python/refs/heads/master/.generator/schemas/v2/openapi.yaml"
      },
      "auth": {
        "type": "api-key",
        "headers": ["DD-API-KEY", "DD-APPLICATION-KEY"],
        "env": {
          "DD_API_KEY": "DD-API-KEY header value",
          "DD_APP_KEY": "DD-APPLICATION-KEY header value",
          "DD_SITE": "Site for API base URL (default: datadoghq.com)"
        }
      },
      "baseUrl": "https://api.${DD_SITE}/api/${version}",
      "docs": "https://docs.datadoghq.com/api/latest/",
      "supportedEndpoints": {
        "metrics": [
          "GET /v1/query",
          "GET /v1/metrics",
          "GET /v1/metrics/{metric_name}",
          "GET /v1/search"
        ],
        "logs": [
          "POST /v2/logs/events/search",
          "POST /v2/logs/analytics/aggregate"
        ]
      }
    }
  }
}
