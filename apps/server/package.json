{
  "name": "@kossabos/server",
  "version": "1.0.0",
  "description": "Kossabos server in TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ENVIRONMENT=dev BUILD_SHA_PREFIX=`git rev-parse --verify HEAD | cut -c1-6` tsx watch src/index.ts",
    "dev:logs": "ENVIRONMENT=dev BUILD_SHA_PREFIX=`git rev-parse --verify HEAD | cut -c1-6` LOG_LEVEL=debug tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern='\\.unit\\.(ts|js)$'",
    "test:integration": "jest --testPathPattern='\\.spec\\.(ts|js)$'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prestart": "npm run build",
    "load-env": "tsx scripts/load-env-from-parameter-store.ts --parameter /aprovan/kossabos/prd/use2/main/env --overwrite"
  },
  "author": "Jacob Sampson",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/client-sns": "^3.490.0",
    "@aws-sdk/client-ssm": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "@kossabos/core": "workspace:^",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.203.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.203.0",
    "@opentelemetry/instrumentation": "^0.203.0",
    "@opentelemetry/instrumentation-pino": "^0.50.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-logs": "^0.203.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "aws-lambda": "^1.0.7",
    "chalk": "^5.3.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.1",
    "koa-pino-logger": "^4.0.0",
    "pino": "^9.7.0",
    "pino-opentelemetry-transport": "^1.0.1",
    "pino-pretty": "^13.0.0",
    "serverless-http": "^3.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/koa": "^2.14.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-pino-logger": "^3.0.4",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^8.0.11",
    "@types/node": "^20.10.6",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.3"
  }
}
