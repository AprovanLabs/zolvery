{
  "name": "@kossabos/server",
  "version": "1.0.0",
  "description": "Kossabos server in TypeScript",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/app.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern='\\.unit\\.(ts|js)$'",
    "test:integration": "jest --testPathPattern='\\.spec\\.(ts|js)$'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prestart": "npm run build",
    "load-env": "tsx scripts/load-env-from-parameter-store.ts --parameter /aprovan/kossabos/prd/use2/main/env --overwrite"
  },
  "author": "Jacob Sampson",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.15.0",
    "@koa/router": "^12.0.1",
    "koa-bodyparser": "^4.4.1",
    "@koa/cors": "^5.0.0",
    "aws-lambda": "^1.0.7",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/client-ssm": "^3.490.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0",
    "serverless-http": "^3.2.0",
    "dotenv": "^16.3.1",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/koa": "^2.14.0",
    "@types/koa__router": "^8.0.11",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa__cors": "^5.0.0",
    "@types/aws-lambda": "^8.10.130",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/dotenv": "^8.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "tsconfig-paths": "^4.2.0",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2",
    "vite": "^5.0.10",
    "vitest": "^1.1.3"
  }
}
